}
.map .arrow-left,
.map .arrow-right {
  position: absolute;
  top: 286px;
  width: 36px;
  height: 36px;
  background-image: url("<%= asset_path 'assets/map-arrow.png' %>");
  background-repeat: no-repeat;
  background-size: 300px auto;
}
.map .arrow-left {
  position: absolute;
  left: 10px;
  background-position: left top;
  z-index: 5000;
}
.map .arrow-right {
  position: absolute;
  right: 10px;
  background-position: right top;
  z-index: 5002;
}
.map .world-map {
  position: relative;
  width: 921px;
  height: 569px;
  background-image: url("<%= asset_path 'assets/maps.png' %>");
  background-position: left top;
  background-repeat: no-repeat;
  background-size: 921px auto;
  z-index: 100;
  -webkit-transition: all 0.5s ease-in-out;
  -moz-transition: all 0.5s ease-in-out;
  -o-transition: all 0.5s ease-in-out;
  transition: all 0.5s ease-in-out;
}
.map .world-map a {
