  right: 32px;
  top: 32px;
  font-size: 26px;
}
.message-detail .inner {
  margin: 20px 20px;
  min-height: 610px;
  background-color: #fff;
  border-radius: 20px;
  background-image: url("<%= asset_path 'assets/question-bg.png' %>");
  background-repeat: no-repeat;
