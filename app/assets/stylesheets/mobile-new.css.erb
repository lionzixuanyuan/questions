  background-repeat: no-repeat;
  background-position: 37px 13px;
  background-size: 100px auto;
}
.message-detail .inner dl {
  padding-bottom: 30px;
}
.message-detail .inner dt {
  font-weight: bold;
  font-size: 15px;
  padding: 2px 13px 9px;
  cursor: pointer;
}
.message-detail .inner dt .arrow {
  border-right: #666 2px solid;
  border-top: #666 2px solid;
  margin-right: 2px;
  width: 6px;
  height: 6px;
  display: inline-block;
  -webkit-transform: rotate(45deg) translate(-2px);
}
.message-detail .inner dt .q-title {
  display: inline-block;
  width: 26em;
  overflow: hidden;
  /*text-overflow:ellipsis;*/
  white-space: nowrap;
}
.message-detail .inner dt.open .arrow {
  -webkit-transform: rotate(135deg) translateY(2px);
}
.message-detail .inner dt.open .q-title {
  width: auto;
  white-space: normal;
}
.message-detail .inner dt.open .buttons {
  display: block;
}
